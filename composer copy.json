{
    "name": "foxdeli/api-php-sdk",
    "description": "Foxdeli API PHP SDK",
    "type": "library",
    "require": {
        "php": "7.x.x",
        "guzzlehttp/guzzle": "^6.5",
        "zircote/swagger-php": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Foxdeli\\ApiPhpSdk\\": "src/",
            "Foxdeli\\OpenApi\\": "openapi/lib/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpunit/php-code-coverage": "^6.1",
        "phpstan/phpstan": "^1.4",
        "nette/neon": "^3.3"
    },
    "scripts": {
        "phpstan": [
            "api-php-sdk/vendor/bin/phpstan analyse -l 8 -c api-php-sdk/phpstan.neon api-php-sdk/lib/ api-php-sdk/tests/"
        ],
        "tests": [
            "api-php-sdk/vendor/bin/phpunit tests"
        ]
    }
}
